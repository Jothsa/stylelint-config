{
  "name": "@jothsa/stylelint-config",
  "scope": "@jothsa",
  "version": "0.1.0",
  "description": "Jothsa's Stylelint config",
  "keywords": [
    "stylelint",
    "stylelint-config"
  ],
  "type": "module",
  "main": "index.js",
  "scripts": {
    "lint:es": "eslint .",
    "lint:prettier": "prettier --cache --plugin-search-dir . --check .",
    "lint": " pnpm lint:prettier; pnpm lint:es",
    "format:es": "eslint . --fix",
    "format:prettier": "prettier --cache --plugin-search-dir . --write .",
    "format": "pnpm format:prettier; pnpm format:es;"
  },
  "peerDependencies": {
    "stylelint": ">=15.0.0"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-json-schema-validator": "^4.8.3",
    "eslint-plugin-jsonc": "^2.13.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-regexp": "^2.2.0",
    "eslint-plugin-yml": "^1.12.2",
    "prettier": "^3.2.5"
  },
  "dependencies": {
    "@jothsa/stylelint-config-idiomatic-order": "^0.2.0",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-recommended": "^14.0.0",
    "stylelint-high-performance-animation": "^1.10.0"
  },
  "packageManager": "pnpm@8.5.1",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jothsa/stylelint-config.git"
  },
  "author": "Jothsa",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jothsa/stylelint-config/issues"
  },
  "homepage": "https://github.com/jothsa/stylelint-config#readme"
}
